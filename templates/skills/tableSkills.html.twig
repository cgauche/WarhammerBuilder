<tr>
        <th>Nom</th>
        <th>Type</th>
        <th>Caractéristiques</th>
        <th>Spécialités</th>
        <th>Source</th>
        <th style="text-align:right">
                <a class="waves-effect waves-light btn" onclick="openForm()"><i
                        class="material-icons">add_circle_outline</i></a>
        </th>
</tr>
{% for skill in skills %}
        <tr>
                <td>{{skill['name']}}</td>
                <td>{{skill['type']}}</td>
                <td>{{skill['charac']}}</td>
                <td>{{skill['specs']}}</td>
                <td class="width120">{{skill['source']}}</td>
                <td style="text-align:right;" class="width120">
                        <a class="waves-effect waves-light btn" onclick="openForm(this.id)" id="{{skill['id']}}"><i
                                class="material-icons">border_color</i></a>
                        <a class="waves-effect waves-light btn modal-trigger" onclick="delMsg(this.id)" id="{{skill['id']}}"><i
                                class="material-icons">delete</i></a>
                </td>
        </tr>
{% endfor %}