{% for BC in containers %}
        <tr>
                <td>{{BC['name']}}</td>
                <td>{{BC['type']}}</td>
                <td>{{BC['price']}}</td>
                <td>{{BC['clutter']}}</td>
                <td>{{BC['availability']}}</td>
