<tr>
        <th>Nom</th>
        <th>Classe</th>
        <th>Description</th>
        <th>Source</th>
        <th style="text-align:right">
                <a class="waves-effect waves-light btn" onclick="openForm()"><i
                        class="material-icons">add_circle_outline</i></a>
        </th>
</tr>
{% for career in careers %}
        <tr>
                <td>{{career['name']}}</td>
                <td>{{career['classe']}}</td>
                <td>{{career['resume']}}</td>
                <td>{{career['source']}}</td>
                <td style="text-align:right; width: 200px;">
                        <a class="waves-effect waves-light btn" onclick="openForm(this.id)" id="{{career['id']}}"><i
                                class="material-icons">border_color</i></a>
                        <a class="waves-effect waves-light btn modal-trigger" onclick="delMsg(this.id)" id="{{career['id']}}"><i
                                class="material-icons">delete</i></a>
                        <a class="waves-effect waves-light btn modal-trigger" href="{{ path('editRanks',{'id' : career['id']}) }}"><i
                                class="material-icons">more_vert</i></a>
                </td>
        </tr>
{% endfor %}